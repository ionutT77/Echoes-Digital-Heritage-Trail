#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Echoes-Digital-Heritage-Trail/node_modules/.pnpm/@mapbox+polyline@0.2.0/node_modules/@mapbox/polyline/bin/node_modules:/mnt/d/Echoes-Digital-Heritage-Trail/node_modules/.pnpm/@mapbox+polyline@0.2.0/node_modules/@mapbox/polyline/node_modules:/mnt/d/Echoes-Digital-Heritage-Trail/node_modules/.pnpm/@mapbox+polyline@0.2.0/node_modules/@mapbox/node_modules:/mnt/d/Echoes-Digital-Heritage-Trail/node_modules/.pnpm/@mapbox+polyline@0.2.0/node_modules:/mnt/d/Echoes-Digital-Heritage-Trail/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Echoes-Digital-Heritage-Trail/node_modules/.pnpm/@mapbox+polyline@0.2.0/node_modules/@mapbox/polyline/bin/node_modules:/mnt/d/Echoes-Digital-Heritage-Trail/node_modules/.pnpm/@mapbox+polyline@0.2.0/node_modules/@mapbox/polyline/node_modules:/mnt/d/Echoes-Digital-Heritage-Trail/node_modules/.pnpm/@mapbox+polyline@0.2.0/node_modules/@mapbox/node_modules:/mnt/d/Echoes-Digital-Heritage-Trail/node_modules/.pnpm/@mapbox+polyline@0.2.0/node_modules:/mnt/d/Echoes-Digital-Heritage-Trail/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@mapbox/polyline/bin/polyline.bin.js" "$@"
else
  exec node  "$basedir/../@mapbox/polyline/bin/polyline.bin.js" "$@"
fi
